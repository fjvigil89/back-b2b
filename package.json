{
  "name": "back-b2b",
  "version": "2.0.8",
  "description": "Backend b2b",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && NODE_ENV=PRODUCTION node -r dotenv/config dist/index.js",
    "dev": "DEBUG=* ts-node-dev --respawn --transpileOnly -r dotenv/config ./src/index.ts",
    "debug": "node --inspect-brk -r ts-node/register -r dotenv/config src/index.ts dotenv_config_path=$(pwd)/.env.dev",
    "test": "TS_NODE_FILES=true npx mocha --timeout 60000  -r ts-node/register -r dotenv/config **/__tests__/*.ts --exit",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build": "npm run clean && npx tsc --skipLibCheck",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/b2b-apps/back-b2b.git"
  },
  "contributors": [
    {
      "name": "Hector Riquelme"
    },
    {
      "name": "Francisco Sanchez",
      "email": "francisco.sanchez@cademsmart.cl"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/b2b-apps/back-b2b/issues"
  },
  "homepage": "https://gitlab.com/b2b-apps/back-b2b#readme",
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/cron": "^1.7.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-jwt": "^0.0.42",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^5.2.7",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.7.5",
    "@types/passport": "^1.0.0",
    "@types/sharp": "^0.22.2",
    "aws-sdk": "^2.502.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "dotenv": "^8.0.0",
    "expo-server-sdk": "^3.4.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mssql": "^5.1.0",
    "multer": "^1.4.2",
