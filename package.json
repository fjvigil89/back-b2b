    "@types/superagent": "^4.1.3",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.18.0",
    "typescript-eslint-parser": "^22.0.0"
  }
